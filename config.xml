<?xml version='1.0' encoding='utf-8'?>
<widget id="com.klarna.sdk.cordova.example" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>KCOCordovaExampleApp</name>
    <description>
        An example Cordova app that uses Klarna Checkout SDK.
    </description>
    <author email="marat.dyatko@klarna.com" href="https://github.com/klarna/kco-mobile-sdk">
        Klarna Checkout Team
    </author>
    <content src="https://www.klarnacheckout.com/" />
    <access origin="*" requires-forward-secrecy="false" />
    <allow-navigation href="http://*/*" requires-forward-secrecy="false" />
    <allow-navigation href="https://*/*" requires-forward-secrecy="false" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="default" />
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
